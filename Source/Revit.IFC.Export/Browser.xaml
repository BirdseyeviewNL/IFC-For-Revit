<Page x:Class="Revit.IFC.Export.Browser"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Revit.IFC.Export"
	  xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="370"
      Title="Browser">

    <Grid>
		<StackPanel>
		<Menu IsMainMenu="True">
			<MenuItem Header="IfcType" Click="MenuItem_Click">
				
			</MenuItem></Menu>
			<xctk:PropertyGrid x:Name="mPropertyGrid" Width="350" Margin="10" >
